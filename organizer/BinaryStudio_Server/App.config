<configuration>
  <connectionStrings>
    <add name="ApplicationServices"
         connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=BSOrganizerDataBase;Integrated Security=true;"
         providerName="System.Data.SqlClient"/>

  </connectionStrings>
